#!/usr/bin/env powscript


assert_server_configuration_relevant()
  debug 'Ensure configs has been genereated by relevant installer'
  if isset $SKIP_CHECKS
    debug "SKIP: Ð°ctual check of installer version in ${INVENTORY_FILE} disabled"
  else
    if is_exists_file ${INVENTORY_FILE} && grep -q "^installer_version=${SCRIPT_VERSION}\$" ${INVENTORY_FILE}
      debug "Configs has been generated by recent version of installeri ${SCRIPT_VERSION}"
    else
      fail "$(translate 'errors.reconfigure_keitaro')"
